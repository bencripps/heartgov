//- div(class='#{prefix}modal')
div(class='modal fade #{prefix}modal')
    div(class='modal-dialog')
        div(class='modal-content')
            div(class='modal-header')
                button(type='button' class='close' data-dismiss='modal' aria-label='Close')
                    span(aria-hidden='true')
                h4(class='modal-title #{prefix}modal-title') <3 Gov Message
            div(class='modal-body #{prefix}modal-body-all' style='text-align:center;')
                span
            div(class='modal-footer')
                button(class='btn btn-danger #{prefix}modal-text-delete' data-dismiss='modal' style='display:none;') delete
                button(class='btn btn-default #{prefix}modal-close' data-dismiss='modal') close
